            <Unit>[g]</Unit>
            <ColumnWidth>15.0</ColumnWidth>
            <CellStyle>general</CellStyle>
        </Column>
        <Column id="i">
            <ReadableName>Node</ReadableName>
            <Description>Name of node</Description>
            <Unit>[i]</Unit>
            <ColumnWidth>15.0</ColumnWidth>
