            <ColumnWidth>19.0</ColumnWidth>
            <CellStyle>rightInt</CellStyle>
        </Column>
        <Column id="pFOMCost">
            <ReadableName>Fixed OM Cost</ReadableName>
            <Description>Per anno fixed O&amp;M cost</Description>
            <Unit>[M€]</Unit>
            <ColumnWidth>15.0</ColumnWidth>
            <CellStyle>rightFloat1</CellStyle>
        </Column>
        <Column id="pInvestCost">
            <ReadableName>Investment Cost</ReadableName>
            <Description>Equivalent per anno investment cost</Description>
            <Unit>[M€]</Unit>
            <ColumnWidth>17.0</ColumnWidth>
            <CellStyle>rightFloat1</CellStyle>
        </Column>
        <Column id="pPmax">
            <ReadableName>Pmax</ReadableName>
            <Description>Maximum active power transfer</Description>
            <Unit>[MW]</Unit>
            <ColumnWidth>15.0</ColumnWidth>
            <CellStyle>rightInt</CellStyle>
        </Column>
        <Column id="pRatio">
            <ReadableName>Tap Ratio</ReadableName>
            <Description>Transformer ratio</Description>
            <Unit>[p.u.]</Unit>
            <ColumnWidth>15.0</ColumnWidth>
            <CellStyle>rightFloat4</CellStyle>
