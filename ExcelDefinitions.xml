            <Unit>[%, 0-1]</Unit>
            <ColumnWidth>15.0</ColumnWidth>
            <CellStyle>rightFloat2</CellStyle>
        </Column>
        <Column id="EnableInvest">
            <ReadableName>EnableInvest</ReadableName>
            <Description>Whether the model can invest in the unit (1) or not (0)</Description>
            <Unit>[0, 1]</Unit>
            <ColumnWidth>15.0</ColumnWidth>
            <CellStyle>rightInt</CellStyle>
        </Column>
        <Column id="excl">
            <ReadableName>Excl.</ReadableName>
            <Description/>
            <Unit/>
            <ColumnWidth>4.86</ColumnWidth>
            <CellStyle>bold</CellStyle>
