    <Columns> <!-- NOTE: Columns are sorted alphabetically (by id) -->
        <Column id="c">
            <ReadableName>Circuit / Name</ReadableName>
            <Description>Name of line (or circuit)</Description>
            <Unit>[c]</Unit>
            <ColumnWidth>15.0</ColumnWidth>
            <CellStyle>general</CellStyle>
        </Column>
        <Column id="CO2Emis">
            <ReadableName>CO2Emis</ReadableName>
            <Description>Specific CO2 emissions of the unit</Description>
            <Unit>[tCO2/MWh]</Unit>
            <ColumnWidth>15.0</ColumnWidth>
            <CellStyle>rightFloat4</CellStyle>
        </Column>
        <Column id="CommitConsumption">
            <ReadableName>CommitConsumption</ReadableName>
            <Description>Consumption while unit is turned on (regardless of output)</Description>
            <Unit>[MWh/h]</Unit>
            <ColumnWidth>23.0</ColumnWidth>
            <CellStyle>rightFloat2</CellStyle>
        </Column>
        <Column id="dataPackage">
            <ReadableName>Data Package</ReadableName>
            <Description>Which package this belongs to</Description>
            <Unit>[dataPackage]</Unit>
            <ColumnWidth>23.86</ColumnWidth>
            <CellStyle>general</CellStyle>
        </Column>
        <Column id="dataSource">
            <ReadableName>Data Source</ReadableName>
            <Description>Where the data for the entry comes from</Description>
            <Unit>[dataSource]</Unit>
            <ColumnWidth>23.86</ColumnWidth>
            <CellStyle>general</CellStyle>
        </Column>
        <Column id="Efficiency">
            <ReadableName>Efficiency</ReadableName>
            <Description>Efficiency calculated as 1/input for 1 MWh, i.e. 0.5 (or 50%) if 2 MWh fuel are required for 1 MWh electricity</Description>
            <Unit>[%, 0-1]</Unit>
            <ColumnWidth>28.0</ColumnWidth>
            <CellStyle>rightFloat2</CellStyle>
        </Column>
