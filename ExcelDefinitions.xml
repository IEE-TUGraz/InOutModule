    <Columns>
        <Column id="excl">
            <ReadableName>Excl.</ReadableName>
            <Description/>
            <Unit/>
            <ColumnWidth>4.86</ColumnWidth>
            <CellStyle>bold</CellStyle>
        </Column>
        <Column id="NOEXCL">
            <ReadableName/>
            <Description/>
            <Unit/>
            <ColumnWidth>4.86</ColumnWidth>
            <CellStyle/>
        </Column>
        <Column id="id">
            <ReadableName>Database ID</ReadableName>
            <Description>ID within database</Description>
            <Unit>[db-key]</Unit>
            <ColumnWidth>19.57</ColumnWidth>
            <CellStyle>dbKey</CellStyle>
        </Column>
        <Column id="p">
            <ReadableName>p</ReadableName>
            <Description>Hour of the year</Description>
            <Unit>[p]</Unit>
            <ColumnWidth>19.0</ColumnWidth>
            <CellStyle>general</CellStyle>
        </Column>
        <Column id="rp">
            <ReadableName>rp</ReadableName>
            <Description>Representative period for this hour</Description>
            <Unit>[rp]</Unit>
            <ColumnWidth>19.0</ColumnWidth>
            <CellStyle>general</CellStyle>
        </Column>
        <Column id="pWeight_rp">
            <ReadableName>Representative period's weight</ReadableName>
            <Description>Weight of representative period within full timespan</Description>
            <Unit>[h]</Unit>
            <ColumnWidth>31.14</ColumnWidth>
            <CellStyle>right</CellStyle>
        </Column>
        <Column id="k">
            <ReadableName>k</ReadableName>
            <Description>Representative hour in rp for this hour</Description>
            <Unit>[k]</Unit>
            <ColumnWidth>19.0</ColumnWidth>
            <CellStyle>general</CellStyle>
        </Column>
        <Column id="pWeight_k">
            <ReadableName>Representative hour's weight</ReadableName>
            <Description>Weight of representative hour within representative period</Description>
            <Unit>[h]</Unit>
            <ColumnWidth>31.14</ColumnWidth>
            <CellStyle>right</CellStyle>
        </Column>
        <Column id="dataPackage">
            <ReadableName>Data Package</ReadableName>
            <Description>Which package this belongs to</Description>
            <Unit>[dataPackage]</Unit>
            <ColumnWidth>23.86</ColumnWidth>
            <CellStyle>general</CellStyle>
        </Column>
        <Column id="dataSource">
            <ReadableName>Data Source</ReadableName>
            <Description>Where the data for the entry comes from</Description>
            <Unit>[dataSource]</Unit>
            <ColumnWidth>23.86</ColumnWidth>
            <CellStyle>general</CellStyle>
        </Column>
        <Column id="pWeight_rp">
            <ReadableName>Representative period's weight</ReadableName>
            <Description>Weight of representative period within full timespan</Description>
            <Unit>[h]</Unit>
            <ColumnWidth>31.14</ColumnWidth>
            <CellStyle>general</CellStyle>
        </Column>
        <Column id="i">
            <ReadableName>Name</ReadableName>
            <Description>Name of node</Description>
            <Unit>[i]</Unit>
            <ColumnWidth>15.0</ColumnWidth>
            <CellStyle>general</CellStyle>
        </Column>
        <Column id="z">
            <ReadableName>Zone</ReadableName>
            <Description>Zone in which node lies</Description>
            <Unit>[z]</Unit>
            <ColumnWidth>15.0</ColumnWidth>
            <CellStyle>general</CellStyle>
        </Column>
        <Column id="pBusBaseV">
            <ReadableName>Base Voltage</ReadableName>
            <Description>Base voltage at node</Description>
            <Unit>[kV]</Unit>
            <ColumnWidth>19.0</ColumnWidth>
            <CellStyle>rightInt</CellStyle>
        </Column>
        <Column id="pBusMaxV">
            <ReadableName>Maximum Voltage</ReadableName>
            <Description>Maximum voltage (in % of bae voltage)</Description>
            <Unit>[p.u.]</Unit>
            <ColumnWidth>20.0</ColumnWidth>
            <CellStyle>rightFloat1</CellStyle>
        </Column>
        <Column id="pBusMinV">
            <ReadableName>Minimum Voltage</ReadableName>
            <Description>Minimum voltage (in % of base voltage)</Description>
            <Unit>[p.u.]</Unit>
            <ColumnWidth>20.0</ColumnWidth>
            <CellStyle>rightFloat1</CellStyle>
        </Column>
        <Column id="pBusB">
            <ReadableName>Bs</ReadableName>
            <Description>Susceptance B connected to node</Description>
            <Unit>[p.u.]</Unit>
            <ColumnWidth>20.0</ColumnWidth>
            <CellStyle>rightFloat1</CellStyle>
        </Column>
        <Column id="pBusG">
            <ReadableName>Gs</ReadableName>
            <Description>Conductance G connected to node</Description>
            <Unit>[p.u.]</Unit>
            <ColumnWidth>20.0</ColumnWidth>
            <CellStyle>rightFloat1</CellStyle>
        </Column>
        <Column id="pBus_pf">
            <ReadableName>Power Factor</ReadableName>
            <Description>Power factor at node</Description>
            <Unit>[p.u.]</Unit>
            <ColumnWidth>20.0</ColumnWidth>
            <CellStyle>rightFloat2</CellStyle>
        </Column>
        <Column id="YearCom">
            <ReadableName>Commission Year</ReadableName>
            <Description>Year where it is commissioned (1.1.xxxx)</Description>
            <Unit>[Year]</Unit>
            <ColumnWidth>19.5</ColumnWidth>
            <CellStyle>rightInt</CellStyle>
        </Column>
        <Column id="YearDecom">
            <ReadableName>Decommission Year</ReadableName>
            <Description>Year where it is decommissioned (31.12.xxxx)</Description>
            <Unit>[Year]</Unit>
            <ColumnWidth>19.5</ColumnWidth>
            <CellStyle>rightInt</CellStyle>
        </Column>
        <Column id="lat">
            <ReadableName>Latitude</ReadableName>
            <Description>Latitude</Description>
            <Unit>[°]</Unit>
            <ColumnWidth>15.0</ColumnWidth>
            <CellStyle>rightFloat4</CellStyle>
        </Column>
        <Column id="lon">
            <ReadableName>Longitude</ReadableName>
            <Description>Longitude</Description>
            <Unit>[°]</Unit>
            <ColumnWidth>15.0</ColumnWidth>
            <CellStyle>rightFloat4</CellStyle>
        </Column>
        <Column id="zoi">
            <ReadableName>Zone of Interest</ReadableName>
            <Description>Whether node is relevant for end result (1) or not (0)</Description>
            <Unit>[0, 1]</Unit>
            <ColumnWidth>19.3</ColumnWidth>
            <CellStyle>right</CellStyle>
        </Column>
    </Columns>

    <PivotColumns>
