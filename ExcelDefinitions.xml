            <ColumnWidth>15.0</ColumnWidth>
            <CellStyle>rightFloat4</CellStyle>
        </Column>
        <Column id="lon">
            <ReadableName>Longitude</ReadableName>
            <Description>Longitude</Description>
            <Unit>[°]</Unit>
            <ColumnWidth>15.0</ColumnWidth>
            <CellStyle>rightFloat4</CellStyle>
        </Column>
        <Column id="MaxInvest">
            <ReadableName>MaxInvest</ReadableName>
            <Description>Maximum number of units that can be invested in</Description>
            <Unit>[0-N]</Unit>
            <ColumnWidth>15.0</ColumnWidth>
            <CellStyle>rightInt</CellStyle>
        </Column>
        <Column id="MaxProd">
            <ReadableName>MaxProd</ReadableName>
            <Description>Maximum active power output of the unit</Description>
            <Unit>[MW]</Unit>
            <ColumnWidth>15.0</ColumnWidth>
            <CellStyle>rightFloat1</CellStyle>
        </Column>
        <Column id="MinDownTime">
            <ReadableName>MinDownTime</ReadableName>
            <Description>Minimum time the unit has to be off after a shutdown</Description>
            <Unit>[h]</Unit>
            <ColumnWidth>15.0</ColumnWidth>
            <CellStyle>rightInt</CellStyle>
        </Column>
        <Column id="MinProd">
            <ReadableName>MinProd</ReadableName>
            <Description>Minimum active power output of the unit</Description>
            <Unit>[MW]</Unit>
            <ColumnWidth>15.0</ColumnWidth>
            <CellStyle>rightFloat1</CellStyle>
        </Column>
        <Column id="MinUpTime">
            <ReadableName>MinUpTime</ReadableName>
            <Description>Minimum time the unit has to be on after a startup</Description>
            <Unit>[h]</Unit>
            <ColumnWidth>15.0</ColumnWidth>
            <CellStyle>rightInt</CellStyle>
        </Column>
        <Column id="NOEXCL">
            <ReadableName/>
            <Description/>
            <Unit/>
            <ColumnWidth>4.86</ColumnWidth>
            <CellStyle/>
